{
  "name": "Insert Soft Hyphen",
  "version": "1.0.0",
  "extensions": [
      {
          "type": "tiptapExtension",
          "alias": "My.Tiptap.InsertSoftHyphen",
          "name": "Soft Hyphen Tiptap Extension",
          "api": "/App_Plugins/TiptapExtension/insert-soft-hyphen.tiptap-api.js",
          "meta": {
              "icon": "icon-code",
              "label": "Insert Soft Hyphen",
              "group": "#tiptap_extGroup_formatting"
          }
      },
      {
          "type": "tiptapToolbarExtension",
          "kind": "button",
          "alias": "My.Tiptap.Toolbar.InsertSoftHyphen",
          "name": "Soft Hyphen Toolbar Button",
          "api": "/App_Plugins/TiptapExtension/insert-soft-hyphen.tiptap-toolbar-api.js",
          "meta": {
              "icon": "icon-hyphen",
              "label": "Insert Soft Hyphen"
          }
      }
  ]
}